<h1>ブックマーク登録</h1>
<%= form_for @folder_content, url: user_post_folder_contents_path, method: :post do |f| %>
	<p><%= f.collection_select :folder_id, Folder.all, :id, :folder_name %></p>
	<p>リンク　<%= @post.link %></p>
	<p>タイトル　<%= f.text_field :folder_contents_title %></p>
	<p>メモ　<%= f.text_area :memo %></p>
	<p><%= f.submit '登録する' %></p>
<% end %>